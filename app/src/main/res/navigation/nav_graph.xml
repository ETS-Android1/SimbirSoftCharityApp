<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graf"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.simbirsoftsummerworkshop.fragments.ProfileFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_profile"/>
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.simbirsoftsummerworkshop.SplashFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_mainFragment22"
            app:destination="@id/mainFragment2" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment2"
        android:name="com.example.simbirsoftsummerworkshop.MainFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment2_to_profileFragment2"
            app:destination="@id/profileFragment" />
    </fragment>
</navigation>